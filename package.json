  "private": true,
  "author": "Ignite, Inc <hello@ignt.com>",
  "scripts": {
    "dev:web": "vite --config vite.web.config.ts --mode web",
    "coverage": "vitest run --coverage",
    "build": "NODE_OPTIONS='--max-old-space-size=8192' vite build --mode production",
    "dev": "NODE_OPTIONS='--max-old-space-size=8192' vite build --mode development",
    "dev:watch": "NODE_OPTIONS='--max-old-space-size=8192' vite build --watch --mode development & NODE_OPTIONS='--max-old-space-size=8192' vite -c vite.scripts.config.js build --watch --mode development",
    "lint": "eslint -c .eslintrc.js --fix  'src/**/*.{ts,js,tsx,vue}'",
    "postbuild": "NODE_OPTIONS='--max-old-space-size=8192' vite -c vite.scripts.config.js build --mode production",
    "postdev": "NODE_OPTIONS='--max-old-space-size=8192' vite -c vite.scripts.config.js build --mode development",
    "postinstall": "git submodule init",
    "prepare": "simple-git-hooks",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "typecheck": "vue-tsc --noEmit"
  },
